	<queryString><![CDATA[select nombre_cta, saldo_calculado
from
 rf_tr_saldos_diarios sd, rf_tr_cuentas_bancarias cb
where 
 cb.id_cuenta in (1052,1053,1054,1058) and 
 (trunc(cb.FECHA_FIN_OPERACION) >= to_date($P!{FECHA_FINAL},'dd/mm/yyyy') or cb.FECHA_FIN_OPERACION is null)  and 
 sd.id_cuenta = cb.id_cuenta and 
 trunc(sd.fecha) = to_date($P!{FECHA_FINAL},'dd/mm/yyyy')
order by cb.unidad_ejecutora]]></queryString>

